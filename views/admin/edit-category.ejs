<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Edit Category</h4>
        <form action="/admin/edit-category/<%=category._id%>" method="post">
          <p id="err" class="text-danger"></p>
          <div class="form-group">
            <p class="text-danger text-center"></p>
            <label>Category</label>
            <input type="text" class="form-control form-control-lg" placeholder="category name" aria-label="Username" name="name" id="value" value="<%=category.name%>">
          </div>
          <button type="submit" class="btn btn-primary ml-3 mr-2 col-md-2">Submit</button>
         </form>
      </div>
    </div>
  </div>